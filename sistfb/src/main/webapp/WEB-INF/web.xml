<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ControllerLogin</servlet-name>
  	<display-name>ControllerLogin</display-name>
  	<description></description>
  	<servlet-class>com.cablesfb.controller.ControllerLogin</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StockController</servlet-name>
  	<display-name>StockController</display-name>
  	<description></description>
  	<servlet-class>com.cablesfb.controller.StockController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SearcherController</servlet-name>
  	<display-name>SearcherController</display-name>
  	<description></description>
  	<servlet-class>com.cablesfb.controller.SearcherController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ControllerLogin</servlet-name>
  	<url-pattern>/ControllerLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StockController</servlet-name>
  	<url-pattern>/StockController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SearcherController</servlet-name>
  	<url-pattern>/SearcherController</url-pattern>
  </servlet-mapping>
</web-app>
